{
  "root": true,
  "env": {
    "node": true
  },
  "extends": [
    "plugin:vue/vue3-essential",
    "@vue/airbnb",
    "@vue/typescript/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "max-len": "off",
    "import/prefer-default-export": "off",
    "vuejs-accessibility/form-control-has-label": "off"
  }
}